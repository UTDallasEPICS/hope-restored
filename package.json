{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "admin": "cd admin && npm run dev",
    "both": "node ./scripts/runAdminAndPublic",
    "cpenv": "node ./scripts/cpenv.js",
    "installDeps": "npm install && cd webcomponent && npm install && cd ../admin && npm install",
    "prismaMigrate": "cd admin && npx prisma migrate dev",
    "prismaResetMigrations": "cd admin && npx prisma migrate reset",
    "prismaStudio": "cd admin && npx prisma studio",
    "public": "cd webcomponent && npm run dev",
    "setup": "npm run cpenv && npm run installDeps && npm run prismaMigrate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@prisma/client": "^5.21.1",
    "@tailwindcss/postcss": "^4.0.17",
    "chart.js": "^3.9.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "jsbarcode": "^3.11.6",
    "nuxt": "^3.16.0",
    "pinia": "^3.0.1",
    "postcss": "^8.5.3",
    "qrcode.vue": "^3.6.0",
    "quagga": "^0.12.1",
    "tailwindcss": "^4.0.17",
    "vite": "^5.4.14",
    "vue": "^3.2.0",
    "vue-chartjs": "^4.1.2",
    "vue3-qr-reader": "^1.0.0"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@sidebase/nuxt-auth": "^0.6.7",
    "@types/node": "^22.7.5",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vue-tsc": "^2.1.6"
  }
}
